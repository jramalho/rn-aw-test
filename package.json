{
  "name": "rn-aw-test",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:release": "react-native run-android --mode=release",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace rnAwTest.xcworkspace -scheme rnAwTest -configuration Release -destination generic/platform=iOS -archivePath build/rnAwTest.xcarchive archive",
    "clean": "react-native clean-project-auto",
    "clean:android": "cd android && ./gradlew clean",
    "clean:ios": "cd ios && rm -rf build && rm -rf ~/Library/Developer/Xcode/DerivedData/rnAwTest-*",
    "ios": "react-native run-ios",
    "ios:release": "react-native run-ios --mode Release",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "ios:pods": "cd ios && bundle exec pod install",
    "prepare": "husky install",
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e:build:android": "detox build --configuration android.emu.release",
    "test:e2e:test:android": "detox test --configuration android.emu.release",
    "test:e2e:build:ios": "detox build --configuration ios.sim.release",
    "test:e2e:test:ios": "detox test --configuration ios.sim.release",
    "type-check": "tsc --noEmit",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output bundle-analysis/index.android.bundle --assets-dest bundle-analysis/",
    "bundle:ios": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output bundle-analysis/index.ios.bundle --assets-dest bundle-analysis/",
    "analyze-bundle": "./scripts/analyze-bundle.sh"
  },
  "dependencies": {
    "@notifee/react-native": "^9.1.8",
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native/new-app-screen": "0.82.0",
    "@react-navigation/bottom-tabs": "^7.0.11",
    "@react-navigation/native": "^7.0.15",
    "@react-navigation/stack": "^7.0.14",
    "react": "19.1.1",
    "react-native": "0.82.0",
    "react-native-biometrics": "^3.0.1",
    "react-native-gesture-handler": "^2.20.2",
    "react-native-paper": "^5.12.5",
    "react-native-reanimated": "4.1.3",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.1.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-worklets": "^0.6.1",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "^20.0.2",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.82.0",
    "@react-native/eslint-config": "0.82.0",
    "@react-native/metro-config": "0.82.0",
    "@react-native/typescript-config": "0.82.0",
    "@testing-library/react-native": "^12.8.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.1",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^19.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "detox": "^20.28.0",
    "eslint": "^8.19.0",
    "husky": "^9.1.6",
    "jest": "^29.6.3",
    "lint-staged": "^15.2.10",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.1",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
